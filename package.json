{
  "name": "haxe-redux-todo-app",
  "version": "0.0.0",
  "scripts": {
    "clean": "shx rm -rf bin",
    "libs": "browserify scripts/libs.js -o res/libs.js -d",
    "serve": "livereloadx -s bin -p $npm_package_config_livereload_port",
    "prepare": "node scripts/prepare.js"
  },
  "dependencies": {
    "haxe-modular": "^0.1.7",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "livereloadx": "^0.3.9",
    "shelljs": "^0.7.8",
    "shx": "^0.2.0"
  },
  "config": {
    "livereload:port": "35927",
    "www-dir": "bin",
    "resources-dir": "res"
  }
}
