{
  "name": "haxe-redux-todo-app",
  "version": "0.0.0",
  "scripts": {
    "build": "cross-env NODE_ENV=development webpack",
    "build:watch": "nodemon -w src/ -e hx -x 'haxe build.hxml'",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "build:cordova:android": "cross-env NODE_ENV=development TARGET=cordova_android webpack && cd cordova && cordova run android",
    "build:cordova:android:prod": "cross-env NODE_ENV=production TARGET=cordova_android webpack && cd cordova && cordova run android",
    "start": "nodemon -w webpack.config.js -x './node_modules/.bin/webpack-dev-server --host 0.0.0.0'",
    "test": "haxe test.hxml -D autotest_suite=$SUITE && node .build/test.js",
    "test:watch": "nodemon -w src/ -e hx -x 'haxe test.hxml -D autotest_suite=$SUITE && node .build/test.js'"
  },
  "dependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.3.1",
    "cordova": "^8.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "haxe-loader": "^0.7.3",
    "html-webpack-plugin": "^2.29.0",
    "jsdom": "^11.6.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.13.3",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.10.0"
  }
}
